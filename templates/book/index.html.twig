<table>
        <tr>
        <th>Id</th>
        <th>title</th>
        <th>PublicationDate</th>
        <th>Enabled</th>
        <th>category</th>
        <th>delete</th>
        <th>authorEmail</th>
        </tr>
    
    {% for books in books %}

    <tr>
    <td>{{books.id}}</td>
    <td>{{books.title}}</td>
    <td>{{books.publicationDate | date('Y-m-d')}}</td>
    <td>{{books.enabled}}</td>
     <td>{{books.category}}</td>
   
    
 <td><a href='{{ path ('app_deleteBook' , { 'id':books.id } ) }}'>delete</a><td>
 <td>{{books.author.email}}</td>
    </tr>

    {% endfor %}
    </table>
